
<IfModule mod_rewrite.c>
	RewriteEngine on
	
	RewriteRule ^debug\.html$ index.php?pages=debug [L]
	RewriteRule ^debug/sites\.json$ index.php?pages=debug&json=sites [L]
	RewriteRule ^debug/social\.json$ index.php?pages=debug&json=social [L]
	RewriteRule ^debug/private\.json$ index.php?pages=debug&json=private [L]
	RewriteRule ^debug/business\.json$ index.php?pages=debug&json=business [L]
	RewriteRule ^debug/images\.json$ index.php?pages=debug&json=images [L]
	RewriteRule ^debug/partner\.json$ index.php?pages=debug&json=partner [L]
	RewriteRule ^debug/sponsor\.json$ index.php?pages=debug&json=sponsor [L]
	RewriteRule ^debug/credits\.json$ index.php?pages=debug&json=credits [L]
	
	########## debug - English ##########
	RewriteRule ^en/debug\.html$ debug.php?lang=en&pages=debug [L]
