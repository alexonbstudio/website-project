RewriteRule ^fr/politique-de-confidentialité\.html$ law.php?lang=fr&pages=law&full=policy-privacy [L]
RewriteRule ^fr/rgpd\.html$ law.php?lang=fr&pages=law&full=rgpd [L]


########## SPONSOR - French ##########
RewriteRule ^fr/sponsorisé\.html$ sponsor.php?lang=fr&pages=index [L]




########## PARTNER - French ##########
RewriteRule ^fr/partenaire\.html$ partner.php?lang=fr&pages=index [L]





########## CONTACT - French ##########
RewriteRule ^fr/contactez\.html$ email.php?lang=fr&pages=index [L]




########## BLOCK - French ##########
RewriteRule ^fr/contact/success\.html$ block.php?lang=fr&pages=success [L]
RewriteRule ^fr/erreur\.html$ block.php?lang=fr&pages=error [L]


####################################################################################################
####################################################################################################
####################################################################################################
####################################################################################################

#Translate English
RewriteRule ^en/home.html$ general.php?lang=en&pages=index [L]


