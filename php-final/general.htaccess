RewriteRule ^en/grpd\.html$ law.php?lang=en&pages=law&full=rgpd [L]


########## SPONSOR - English ##########
RewriteRule ^en/sponsor\.html$ sponsor.php?lang=en&pages=index [L]





########## PARTNER - English ##########
RewriteRule ^en/partner\.html$ partner.php?lang=en&pages=index [L]





########## CONTACT - English ##########
RewriteRule ^en/contact\.html$ email.php?lang=en&pages=index [L]




