<IfModule mod_rewrite.c>
RewriteEngine on
#Translate French
RewriteRule ^fr/accueil.html$ general.php?lang=fr&pages=index [L]

####################################################################################################
####################################################################################################
####################################################################################################
####################################################################################################

########## LAW - French ########## 
RewriteRule ^fr/règles\.html$ law.php?lang=fr&pages=law [L]
RewriteRule ^fr/conditions-générales-utilisation\.html$ law.php?lang=fr&pages=law&full=cgu [L]
RewriteRule ^fr/conditions-générales-de-vente\.html$ law.php?lang=fr&pages=law&full=cgv [L]
RewriteRule ^fr/dmca\.html$ law.php?lang=fr&pages=law&full=dmca [L]
